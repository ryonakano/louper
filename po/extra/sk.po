msgstr "Pridaný preklad do francúzštiny (vďaka @NathanBnm)"

#: data/louper.appdata.xml.in:48 data/louper.appdata.xml.in:56
msgid "Update Japanese translation"
msgstr "Aktualizácia prekladu do japončiny"

#: data/louper.appdata.xml.in:55
msgid "Now you can quit the app with the shortcut key Ctrl+Q"
msgstr "Teraz môžete ukončiť aplikáciu pomocou klávesovej skratky Ctrl+Q"

